{
  "name": "backend-clean-architecture-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "env-cmd -e development ts-node-dev -r tsconfig-paths/register src/app/main.ts",
    "prebuild": "rimraf build",
    "build": "webpack --config webpack.config.ts",
    "postbuild": "ts-node-script scripts/postbuild.ts",
    "lint": "eslint \"{src,scripts}/**/*.ts\" --fix",
    "format": "prettier --write src",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.22.0",
    "fastify-compress": "^3.6.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/supertest": "^2.0.11",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "copy-webpack-plugin": "^9.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
